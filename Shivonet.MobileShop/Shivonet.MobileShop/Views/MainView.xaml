<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:views="clr-namespace:Shivonet.MobileShop.Core.Views"
             x:Class="Shivonet.MobileShop.Core.Views.MainView"
             Title="Opabid Farms"
                  xmlns:utility="clr-namespace:Shivonet.MobileShop.Core.Utility;assembly=Shivonet.MobileShop.Core"
                  utility:ViewModelLocator.AutoWireViewModel="True"
             MasterBehavior="Popover"
                  >
    <MasterDetailPage.Master>
        <views:MenuView BindingContext="{Binding MenuViewModel}" />
    </MasterDetailPage.Master>
    <MasterDetailPage.Detail>
        <ContentPage></ContentPage>
    </MasterDetailPage.Detail>
</MasterDetailPage>